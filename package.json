{
  "name": "webpack-boilerplate",
  "version": "1.0.0",
  "description": "Webpack boilerplate with Babel, Sass, Autoprefixer, Handlebars",
  "author": "Onur Kerimov <onurkerimov@gmail.com>",
  "main": "./src/js/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --mode development",
